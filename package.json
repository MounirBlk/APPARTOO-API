{
    "name": "api",
    "version": "1.0.0",
    "description": "APPARTOO API PANGOLINS",
    "main": "dist/app.js",
    "scripts": {
        "start": "pm2 kill && set ENV=PROD && pm2 start dist/app.js && pm2 logs",
        "serve": "pm2 kill && set ENV=DEV && npm run clean && npm run build && pm2 start dist/app.js && pm2 logs",
        "dev": "pm2 kill && nodemon app.ts",
        "clean": "rimraf ./dist",
        "build": "tsc -p ./tsconfig.json && npm run copy-assets",
        "copy-assets": "ts-node copyAssets.ts",
        "postinstall": "npm run build",
        "test": "set ENV=PROD && jasmine",
        "test:coverage": "set ENV=PROD && nyc --reporter=lcov --reporter=html --reporter=text-summary jasmine",
        "test:local": "set ENV=TEST && rimraf ./dist && npm run build && jasmine",
        "test:local:coverage": "set ENV=TEST && rimraf ./dist && npm run build && nyc --reporter=lcov --reporter=html --reporter=text-summary jasmine",
        "git:sync": "git checkout main && git fetch -p && git pull origin main && git branch",
        "git:sync:dev": "npm run git:sync && git branch -D develop && git checkout -b develop && git branch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MounirBlk/APPARTOO-API.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/MounirBlk/APPARTOO-API/issues"
    },
    "homepage": "https://github.com/MounirBlk/APPARTOO-API#readme",
    "dependencies": {
        "async": "^3.2.0",
        "bluebird": "^3.7.2",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "express-rate-limit": "^5.2.6",
        "get-port": "^6.0.0",
        "jasmine": "^3.10.0",
        "jasmine-ts-console-reporter": "^3.1.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mime-types": "^2.1.30",
        "mongoose": "^5.11.15",
        "mongoose-bcrypt": "^1.9.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "nodemailer": "^6.4.18",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "passport": "^0.4.1",
        "pm2": "^4.5.5",
        "progress": "^2.0.3",
        "rimraf": "^3.0.2",
        "sharp": "^0.27.2",
        "socket.io": "^4.0.1",
        "supertest": "^6.1.6",
        "uuid": "^8.3.2",
        "validator": "^13.6.0",
        "winston": "^3.5.1",
        "axios": "^0.21.4",
        "cron": "^1.8.2",
        "shelljs": "^0.8.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.2.2"
    },
    "devDependencies": {
        "@types/async": "^3.2.5",
        "@types/cors": "^2.8.10",
        "@types/cron": "^1.7.2",
        "@types/ejs": "^3.0.6",
        "@types/jasmine": "^3.10.2",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.168",
        "@types/mime-types": "^2.1.0",
        "@types/mongodb": "^3.6.8",
        "@types/mongoose": "^5.10.3",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.5",
        "@types/progress": "^2.0.3",
        "@types/sharp": "^0.27.2",
        "@types/socket.io": "^2.1.13",
        "@types/uuid": "^8.3.0",
        "@types/validator": "^13.1.3",
        "@types/bluebird": "^3.5.33",
        "@types/express": "^4.17.11",
        "@types/express-rate-limit": "^5.1.1",
        "@types/node": "^14.14.31",
        "@types/nodemailer": "^6.4.0",
        "@types/passport": "^1.0.6",
        "@types/shelljs": "^0.8.8",
        "@types/supertest": "^2.0.11",
        "@types/winston": "^2.4.4"
    }
}